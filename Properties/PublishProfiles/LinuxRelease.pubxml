<Project>
  <PropertyGroup>
    <PublishProfile>LinuxRelease</PublishProfile>
    <PublishDir>bin\Release\net8.0\publish\linux-x64\</PublishDir>
    <Configuration>Release</Configuration>
    <TargetFramework>net8.0</TargetFramework>
    <RuntimeIdentifier>linux-x64</RuntimeIdentifier>
    <SelfContained>false</SelfContained>
    <PublishTrimmed>false</PublishTrimmed>
    <PublishSingleFile>false</PublishSingleFile>
    <DeleteExistingFiles>true</DeleteExistingFiles>
    <CopyAllFilesToSingleFolderForPackageDependsOn>_CopyAppConfigFile</CopyAllFilesToSingleFolderForPackageDependsOn>
  </PropertyGroup>
</Project>
